{
  "name": "@caldwell619/mui-inline-suggest",
  "version": "1.3.0",
  "description": "Material UI TextField component that will suggest the rest of the input, like an IDE or terminal",
  "repository": "christopher-caldwell/mui-inline-suggest",
  "license": "MIT",
  "author": "christopher-caldwell",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s src -o dist",
    "bump:version": "standard-version",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "publish:version": "sh scripts/publish.sh"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "@types/node": "^20.2.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.4.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^5.0.1",
    "standard-version": "^9.5.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.6.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/material": ">=5",
    "react": ">=16.0.0",
    "react-dom": ">=16"
  }
}
